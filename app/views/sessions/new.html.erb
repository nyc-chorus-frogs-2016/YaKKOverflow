<%= form_tag session_path do %>
<fieldset>
  <legend>log in to YaKK Overflow</legend>
  <ls>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </ls>
  <ls>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </ls>
  <ls>
    <%= submit_tag 'Log in' %>
  </ls>
</fieldset>

<% end %>
